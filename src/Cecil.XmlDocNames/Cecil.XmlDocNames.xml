<?xml version="1.0"?>
<doc>
    <assembly>
        <name>Cecil.XmlDocNames</name>
    </assembly>
    <members>
        <member name="T:Cecil.XmlDocNames.MemberReferenceExtensions">
            <summary>
            Provides extension methods for <see cref="T:Mono.Cecil.MemberReference">MemberReference</see>.
            </summary>
        </member>
        <member name="M:Cecil.XmlDocNames.MemberReferenceExtensions.GetXmlDocName(Mono.Cecil.MemberReference)">
            <summary>
            Gets the name of a <see cref="T:Mono.Cecil.MemberReference">MemberReference</see> in the same format used by XML documentation.
            </summary>
            <param name="this">The <see cref="T:Mono.Cecil.MemberReference">MemberReference</see> instance on which this method is called.</param>
            <returns>A <see cref="T:System.String">string</see> containing the member's name  in the same format used by XML documentation.</returns>
            <exception cref="T:System.InvalidOperationException">
            <para><paramref name="this"/> is not an instance of one of the following classes:</para>
            <list type="bullet">
              <item>
                <term><see cref="T:Mono.Cecil.TypeReference"/></term>
                <description>A type reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.MethodReference"/></term>
                <description>A method reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.PropertyReference"/></term>
                <description>A property reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.FieldReference"/></term>
                <description>A field reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.EventReference"/></term>
                <description>An event reference or definition.</description>
              </item>
            </list>
            </exception>
            <remarks>
            <para><paramref name="this"/> MUST NOT be <c>null</c>.</para>
            </remarks>
        </member>
        <member name="T:Cecil.XmlDocNames.StringBuilderExtensions">
            <summary>
            Provides extension methods for <see cref="T:System.Text.StringBuilder">StringBuilder</see>.
            </summary>
        </member>
        <member name="M:Cecil.XmlDocNames.StringBuilderExtensions.AppendXmlDocName(System.Text.StringBuilder,Mono.Cecil.MemberReference)">
            <summary>
            Appends the name of a <see cref="T:Mono.Cecil.MemberReference">MemberReference</see> to a <see cref="T:System.Text.StringBuilder">StringBuilder</see>,
            in the same format used by XML documentation.
            </summary>
            <param name="this">The <see cref="T:System.Text.StringBuilder">StringBuilder</see> instance on which this method is called.</param>
            <param name="member">The <see cref="T:Mono.Cecil.MemberReference">MemberReference</see> whose name will be appended to <paramref name="this"/>.</param>
            <returns><paramref name="this"/></returns>
            <exception cref="T:System.InvalidOperationException">
            <para><paramref name="member"/> is not an instance of one of the following classes:</para>
            <list type="bullet">
              <item>
                <term><see cref="T:Mono.Cecil.TypeReference"/></term>
                <description>A type reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.MethodReference"/></term>
                <description>A method reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.PropertyReference"/></term>
                <description>A property reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.FieldReference"/></term>
                <description>A field reference or definition.</description>
              </item>
              <item>
                <term><see cref="T:Mono.Cecil.EventReference"/></term>
                <description>An event reference or definition.</description>
              </item>
            </list>
            </exception>
            <remarks>
            <para><paramref name="this"/> MUST NOT be <c>null</c>.</para>
            <para><paramref name="member"/> MUST NOT be <c>null</c>.</para>
            </remarks>
        </member>
    </members>
</doc>
